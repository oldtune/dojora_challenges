{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cargo",
      "command": "run",
      "problemMatcher": ["$rustc"],
      "label": "rust: cargo run"
    },
    {
      "type": "cargo",
      "command": "test",
      "problemMatcher": ["$rustc"],
      "group": "test",
      "label": "rust: cargo test"
    },
    {
      "type": "cargo",
      "command": "build",
      "problemMatcher": "$rustc",
      "isBackground": true,
      "group": "build"
    },
    {
      "type": "cargo",
      "command": "check",
      "problemMatcher": "$rustc",
      "isBackground": true,
      "group": "none"
    }
  ]
}
